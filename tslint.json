{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": [
            true,
            2
        ],
        "semicolon": [
            true,
            "always"
        ],
        "curly": [
            true,
            "ignore-same-line"
        ],
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any",
                "named-imports-order": "case-insensitive"
            }
        ],
        "jsx-boolean-value": [
            "never"
        ]
    },
    "rulesDirectory": []
}